{
    "name": "roblox-lua-autofills",
    "displayName": "Roblox Lua Autocompletes",
    "description": "Autocompletes for Roblox Lua.",
    "version": "1.3.2",
    "publisher": "Kampfkarren",
    "repository": "https://github.com/Kampfkarren/roblox-lua-autofills",
    "engines": {
        "vscode": "^1.37.0"
    },
    "categories": [
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:lua",
        "workspaceContains:*.project.json"
    ],
    "contributes": {
        "configuration": {
            "title": "Roblox Lua Autocompletes",
            "properties": {
                "robloxLuaAutofills.rojo": {
                    "type": "boolean",
                    "description": "Specifies whether to support Rojo specific autocompletes",
                    "default": true
                }
            }
        },

        "snippets": [
            {
                "language": "lua",
                "path": "./snippets.json"
            }
        ]
    },
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.3.0",
        "@types/request-promise-native": "^1.0.15",
        "tslint": "^5.8.0",
        "typescript": "^3.6.2",
        "vscode": "^1.1.36"
    },
    "dependencies": {
        "request": "^2.88.0",
        "request-promise-native": "^1.0.7"
    }
}
